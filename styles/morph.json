					"slug": "md"
				},
				{
					"name": "Large",
					"shadow": "8px 8px 40px rgba(0, 0, 0, 0.15), inset 1px 1px 3px rgba(255, 255, 255, 0.5), inset -5px -5px 20px rgba(0, 0, 0, 0.05)",
					"slug": "lg"
				},
