{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Morph",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#d9e3f1",
					"name": "Base/Body",
					"slug": "base"
				},
				{
					"color": "#7b8ab8",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#d9e3f2",
					"name": "Secondary Body BG",
					"slug": "bs-secondary-bg"
				},
				{
					"color": "rgba(123, 138, 184, 0.9)",
					"name": "Secondary Body Contrast",
					"slug": "bs-secondary-color"
				},
				{
					"color": "#f0f5fa",
					"name": "Tertiary Body BG",
					"slug": "bs-tertiary-bg"
				},
				{
					"color": "rgba(123, 138, 184, 0.8)",
					"name": "Tertiary Body Contrast",
					"slug": "bs-tertiary-color"
				},
				{
					"color": "rgba(55, 94, 148, 0.1)",
					"name": "Border Color",
					"slug": "bs-border-color"
				},
				{
					"color": "#378dfc",
					"name": "Primary",
					"slug": "bs-primary"
				},
				{
					"color": "#d9e3f3",
					"name": "Secondary",
					"slug": "bs-secondary"
				},
				{
					"color": "#43cc29",
					"name": "Success",
					"slug": "bs-success"
				},
				{
					"color": "#5b62f4",
					"name": "Info",
					"slug": "bs-info"
				},
				{
					"color": "#ffc107",
					"name": "Warning",
					"slug": "bs-warning"
				},
				{
					"color": "#e52527",
					"name": "Danger",
					"slug": "bs-danger"
				},
				{
					"color": "#f0f5fb",
					"name": "Light",
					"slug": "bs-light"
				},
				{
					"color": "#212529",
					"name": "Dark",
					"slug": "bs-dark"
				},
				{
					"color": "#d7e8fe",
					"name": "Primary Subtle",
					"slug": "bs-primary-bg-subtle"
				},
				{
					"color": "#333333",
					"name": "Secondary Subtle",
					"slug": "bs-secondary-bg-subtle"
				},
				{
					"color": "#d9f5d4",
					"name": "Success Subtle",
					"slug": "bs-success-bg-subtle"
				},
				{
					"color": "#dee0fd",
					"name": "Info Subtle",
					"slug": "bs-info-bg-subtle"
				},
				{
					"color": "#fff3cd",
					"name": "Warning Subtle",
					"slug": "bs-warning-bg-subtle"
				},
				{
					"color": "#fad3d4",
					"name": "Danger Subtle",
					"slug": "bs-danger-bg-subtle"
				},
				{
					"color": "#f8fafd",
					"name": "Light Subtle",
					"slug": "bs-light-bg-subtle"
				},
				{
					"color": "#bed1e6",
					"name": "Dark Subtle",
					"slug": "bs-dark-bg-subtle"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				},
				{
					"color": "currentcolor",
					"name": "Current",
					"slug": "current"
				},
				{
					"color": "color-mix(in srgb, currentColor 20%, transparent)",
					"name": "Current Mix",
					"slug": "current-mix"
				},
				{
					"color": "inherit",
					"name": "Inherit",
					"slug": "inherit"
				}
			]
		},
		"custom": {
			"sp-replace-string": "all",
			"bs-sp-html-font-size": "18",
			"bs-body-font-weight": "400",
			"bs-heading-font-weight": "700",
			"bs-display-font-weight": "400",
			"bs-btn-font-weight": "700",
			"wp--preset--font-family--body": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--heading": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--display": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--monospace": "var(--wp--preset--font-family--bs-monospace)",
			"bs-border-width": "0",
			"bs-sp-spacing-x": "1.5rem",
			"bs-sp-spacing-y": "1rem",
			"bs-heading-color": "#485785",
			"bs-link-color": "#485785",
			"bs-highlight-color": "#7b8ab8"
		},
		"shadow": {
			"presets": [
				{
					"name": "Small",
					"shadow": "0 0.125rem 0.25rem rgba(55, 94, 148, 0.2)",
					"slug": "sm"
				},
				{
					"name": "Medium",
					"shadow": "5px 5px 10px rgba(55, 94, 148, 0.2), -5px -5px 10px rgba(255, 255, 255, 0.4)",
					"slug": "md"
				},
				{
					"name": "Large",
					"shadow": "8px 8px 40px rgba(0, 0, 0, 0.15)",
					"slug": "lg"
				},
				{
					"name": "Inset",
					"shadow": "inset 2px 2px 8px rgba(55, 94, 148, 0.3), inset -3px -2px 5px rgba(255, 255, 255, 0.8)",
					"slug": "inset"
				},
				{
					"name": "Button",
					"shadow": "5px 5px 10px rgba(55, 94, 148, .2), -5px -5px 10px rgba(255, 255, 255, 0.4)",
					"slug": "button"
				},
				{
					"name": "Button Focus",
					"shadow": "0 0 0 0.25rem rgba(var(--bs-btn-focus-shadow-rgb), .5)",
					"slug": "button-focus"
				},
				{
					"name": "Button Active",
					"shadow": "inset 0 3px 5px rgba(0, 0, 0, 0.125)",
					"slug": "button-active"
				},
				{
					"name": "Theme",
					"shadow": "inset 2px 2px 6px rgba(109, 114, 121, 0.2), inset -3px -2px 4px rgba(255, 255, 255, 0.2)",
					"slug": "theme"
				},
				{
					"name": "Form Focus",
					"shadow": "inset 2px 2px 8px rgba(55, 94, 148, 0.3), inset -3px -2px 5px rgba(255, 255, 255, 0.8), 0 0 0 0.25rem rgba(55, 141, 252, 0.25)",
					"slug": "form-focus"
				},
				{
					"name": "Form Shadow",
					"shadow": "inset 2px 2px 8px rgba(55, 94, 148, 0.3), inset -3px -2px 5px rgba(255, 255, 255, 0.8)",
					"slug": "form-control-shadow"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Nunito Sans, -apple-system, BlinkMacSystemFont, \"Segoe UI\",Roboto, \"Helvetica Neue\",Arial, sans-serif, \"Apple Color Emoji\",\"Segoe UI Emoji\",\"Segoe UI Symbol\"",
					"name": "System San Serif Stack",
					"slug": "system"
				},
				{
					"fontFamily": "Nunito Sans",
					"slug": "nunito-sans",
					"fontFace": [
						{
							"fontFamily": "Nunito Sans",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/nunito/nunito-sans_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Nunito Sans",
							"fontStyle": "normal",
							"fontWeight": "500",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/nunito/nunito-sans_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Nunito Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/nunito/nunito-sans_normal_700.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\",\"Courier New\",monospace",
					"name": "Bootstrap Monospace Stack",
					"slug": "bs-monospace"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|body",
			"fontSize": "var:preset|font-size|medium",
			"fontWeight": "var(--bs-body-font-weight)",
			"letterSpacing": "0.02rem",
			"lineHeight": "1.5"
		},
		"css": "",
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--bs-secondary)",
						"text": "var(--bs-body-color)"
					}
				},
				"border": {
					"color": "transparent",
					"radius": "50rem",
					"width": "var(--bs-btn-border-width)"
				},
				"color": {
					"background": "var(--bs-secondary)",
					"text": "var(--bs-body-color)"
				},
				":focus": {
					"border": {
						"color": "transparent",
						"radius": "50rem",
						"width": "var(--bs-border-width)"
					},
					"color": {
						"background": "var(--bs-secondary)",
						"text": "var(--bs-body-color)"
					}
				},
				":hover": {
					"border": {
						"color": "transparent",
						"radius": "50rem"
					},
					"color": {
						"background": "var(--bs-btn-hover-bg)",
						"text": "var(--bs-btn-hover-color)"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--bs-btn-padding-y)",
						"left": "var(--bs-btn-padding-x)",
						"right": "var(--bs-btn-padding-x)",
						"top": "var(--bs-btn-padding-y)"
					}
				},
				"typography": {
					"fontFamily": "var(--bs-btn-font-family)",
					"fontSize": "var(--bs-btn-font-size)",
					"fontWeight": "var(--bs-btn-font-weight)",
					"lineHeight": "var(--bs-btn-line-height)"
				}
			}
		}
	}
}