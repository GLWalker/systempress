	"styles": {
		"css": "body,\n.alert-secondary{\n--bs-alert-color:var(--wp--preset--color--contrast) !important;\n--bs-secondary-text:var(--wp--preset--color--contrast);\n}\n.alert,\n.dropdown-menu{\n-webkit-backdrop-filter:blur(3px);\nbackdrop-filter:blur(3px);\n}\n\n.btn-group:not(.btn-group > .btn-group),\n.btn-group:not(.dropdown-menu),\n.wp-element-button,\n.btn:not(.wp-block-button),\n.btn-sm>.wp-element-button,\n.btn:not(.wp-block-button).btn-sm,\n.btn-lg>.wp-element-button,\n.btn:not(.wp-block-button).btn-lg{\n--bs-border-radius:50rem;\n--bs-border-radius-sm:50rem;\n--bs-border-radius-lg:50rem;\n}\n.btn-group > .dropdown-menu.show,\n.wp-block-buttons.btn-group-vertical>.wp-block-button>.wp-element-button,\n.wp-block-buttons.btn-group-vertical>.wp-block-button>.wp-element-button:active:hover,\n.wp-block-buttons.btn-group-vertical>.wp-block-button>.wp-element-button:active,\n.wp-block-buttons.btn-group-vertical>.wp-block-button>.wp-element-button:focus,\n.wp-block-buttons.btn-group-vertical>.wp-block-button>.wp-element-button:active:focus,\n.btn-group-vertical,\n.btn-group-vertical .btn,\n.btn-group-vertical .btn:hover,\n.btn-group-vertical .btn:active,\n.btn-group-vertical .btn:focus,\n.btn-group-vertical .btn:active:focus{\n--bs-border-radius:1rem;\n--bs-border-radius-sm:1rem;\n--bs-border-radius-lg:1rem;\n}\n.dark-mode .dropdown-menu,\n.input-group .btn{\n--bs-border-radius:0.375rem;\n--bs-border-radius-sm:.25rem;\n--bs-border-radius-lg:.5rem;\n}\n\n.card-header{\nborder-width:1px;\nborder-color:rgba(55, 94, 148, 0.1) !important;\n}\n\n.card[class*=\"border-\"]>.card-header,\n.card[class*=\"bg-\"],\n.card[class*=\"bg-\"]>.card-header{\nborder-color:rgba(55, 94, 148, 0.1) !important;\n}\n.list-group{\nbackground-color:var(--bs-list-group-bg);\n}\n[class*=\"list-group-item-\"] li,\n.list-group-item[class*=\"list-group-item-\"]{\nbackground-color:transparent;\n}\n.nav-link{\npadding:0.5rem 1rem;\n}\n.nav-tabs .nav-item:first-child .nav-link{\nborder-top-left-radius:0.375rem;\nborder-bottom-left-radius:0.375rem;\n}\n.nav-tabs .nav-item:last-child .nav-link{\nborder-top-right-radius:0.375rem;\nborder-bottom-right-radius:0.375rem;\n}\n\n.wp-block.list-group-item-action li:active,\n.list-group-item-action:active,\n.wp-block.list-group-item-action li:hover,\n.wp-block.list-group-item-action li:focus,\n.list-group-item-action:not(ol):hover,\n.list-group-item-action:not(ol):focus .list-group-item-action:not(ul):hover,\n.list-group-item-action:not(ul):focus{\nbackground-color:transparent;\n}\n.card,\n.btn,\n.btn-group,\n.btn-group-verticle,\n.wp-element-button{\n--bs-primary-text:var(--bs-lighter-text);\n--bs-success-text:var(--bs-lighter-text);\n--bs-warning-text:var(--bs-lighter-text);\n}\n\n[class*=\"btn-outline\"],\n.is-style-outline>.wp-element-button,\n.is-style-outline[class*=\"btn-\"]>.wp-element-button,\n.wp-block-button[class*=\"btn-outline\"]>.wp-element-button,\n.btn[class*=\"btn-outline\"]:not(.wp-block-button){\nborder:calc(2px * 1.8) solid var(--bs-btn-border-color);\nbox-shadow:var(--bs-form-control-shadow);\n}"
	}
