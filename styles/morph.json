			"bs-breadcrumb-bg": "var(--bs-secondary-bg-subtle)",
			"bs-breadcrumb-box-shadow": "var(--bs-box-shadow-button)",
			"bs-btn-color": "var(--wp--preset--color--contrast)",
			"bs-btn-bg": "var(--wp--preset--color--base)",
			"bs-btn-border-color": "none",
			"bs-btn-hover-border-color": "none",
			"bs-btn-box-shadow": "var(--bs-box-shadow-button)",
			"bs-card-border-color": "var(--bs-border-color-translucent)",
			"bs-card-box-shadow": "var(--wp--preset--shadow--theme)",
			"bs-card-cap-bg": "transparent",
			"bs-dropdown-bg": "rgba(var(--bs-tertiary-bg-rgb),.8)",
			"bs-form-control-bg": "rgba(var(--bs-tertiary-bg-rgb),.5)",
			"bs-form-check-bg-color": "var(--bs-dark-bg-subtle)",
