			"bs-list-group-bg": "transparent",
			"bs-list-group-box-shadow": "var(--wp--preset--shadow--theme)",
			"bs-nav-link-color": "var(--bs-heading-color)",
			"bs-nav-link-hover-color": "var(--bs-link-hover-color)",
			"bs-nav-tabs-bg": "var(--bs-secondary-bg-subtle)",
			"bs-nav-tabs-bg-radius": "0.375rem",
			"bs-nav-tabs-bg-shadow": "var(--bs-box-shadow-button)",
			"bs-nav-tabs-border-color": "none",
			"bs-nav-tabs-border-radius": "0",
			"bs-nav-tabs-link-hover-border-color": "none",
			"bs-nav-tabs-link-active-color": "#fff",
			"bs-nav-tabs-link-active-bg": "var(--bs-primary)",
			"bs-nav-tabs-link-active-border-color": "none",
			"bs-nav-tabs-link-active-box-shadow": "var(--bs-box-shadow-inset)",
			"bs-nav-pills-bg": "var(--bs-secondary-bg-subtle)",
			"bs-nav-pills-bg-radius": "0.375rem",
			"bs-nav-pills-bg-shadow": "var(--bs-box-shadow-button)",
			"bs-nav-pills-link-active-color": "#fff",
			"bs-nav-pills-link-active-bg": "var(--bs-primary)",
			"bs-nav-pills-link-active-box-shadow": "var(--bs-box-shadow-inset)",
			"bs-pagination-color": "var(--bs-emphasis-color)",
			"bs-pagination-active-color": "var(--bs-lighter-text)",
			"bs-pagination-active-bg": "var(--bs-primary)",
			"bs-progress-height": "var(--wp--preset--font-size--x-large)",
			"bs-progress-bg": "rgba(var(--bs-tertiary-bg-rgb),.5)",
			"bs-range-thumb-bg": "var(--bs-primary)",
			"bs-range-track-bg": "rgba(var(--bs-tertiary-bg-rgb),.5)"
