			"bs-heading-font-weight": "700",
			"bs-display-font-weight": "400",
			"bs-btn-font-weight": "700",
			"wp--preset--font-family--body": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--heading": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--display": "var(--wp--preset--font-family--system)",
			"wp--preset--font-family--monospace": "var(--wp--preset--font-family--bs-monospace)",
			"bs-border-width": "0",
			"bs-sp-spacing-x": "1.5rem",
			"bs-sp-spacing-y": "1rem",
			"bs-heading-color": "#485785",
			"bs-dm-heading-color": "inherit",
			"bs-alert-color": "var(--bs-emphasis-color)",
			"bs-alert-box-shadow": "var(--bs-box-shadow-lg)",
			"bs-accordion-border-color": "transparent",
			"bs-accordion-active-color": "var(--bs-primary-text-emphasis)",
			"bs-accordion-active-bg": "var(--bs-primary-bg-subtle)",
