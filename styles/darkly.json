{
    "version": 2,
    "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
    "title": "Darkly",
    "settings": {
        "custom": {
            "sp-replace-string": "all",
            "bs-html-text-shadow": "none",
            "bs-body-color-text-shadow": "none",
            "bs-body-bg-text-shadow": "none",
            "bs-secondary-color-text-shadow": "none",
            "bs-secondary-bg-text-shadow": "none",
            "bs-tertiary-color-text-shadow": "var(--bs-light-textshadow)",
            "bs-tertiary-bg-text-shadow": "none",
            "bs-heading-text-shadow": "none",
            "bs-display-text-shadow": "none",
            "bs-single-title-text-shadow": "none",
            "bs-heading-color": "inherit",
            "bs-dm-heading-color": "inherit",
            "bs-link-color": "var(--bs-success)",
            "bs-code-color": "#e83e8c",
            "bs-highlight-color": "#212524",
            "bs-highlight-bg": "#fdebd0",
            "bs-accordion-active-bg": "var(--bs-primary-bg-subtle)",
            "bs-breadcrumb-bg": "var(--bs-secondary)",
            "bs-breadcrumb-divider-color": "var(--bs-lighter-text)",
            "bs-breadcrumb-item-active-color": "var(--bs-lighter-text)",
            "bs-card-color": "var(--bs-lighter-text)",
            "bs-card-bg": "var(--bs-dark)",
            "bs-dropdown-link-hover-bg": "var(--bs-primary)",
            "bs-dropdown-border-color": "var(--bs-border-color)",
            "bs-list-group-bg": "var(--bs-dark)",
            "bs-list-group-action-hover-bg": "var(--bs-secondary)",
            "bs-pagination-color": "var(--bs-lighter-text)",
            "bs-pagination-bg": "var(--bs-success)",
            "bs-pagination-border-color": "var(--bs-success-border-color)",
            "bs-pagination-hover-color": "var(--bs-lighter-text)",
            "bs-pagination-hover-bg": "rgb(var(--bs-success-shadow-rgb))",
            "bs-pagination-hover-border-color": "var(--bs-success-border-color)",
            "bs-pagination-active-color": "var(--bs-lighter-text)",
            "bs-pagination-active-bg": "rgb(var(--bs-success-shadow-rgb))",
            "bs-pagination-active-border-color": "var(--bs-success-border-color)",
            "bs-progress-bg": "var(--bs-secondary)",
            "bs-popover-header-bg": "var(--bs-secondary)",
            "bs-popover-bg": "var(--bs-dark)",
            "bs-nav-tabs-link-active-color": "var(--bs-lighter-text)",
            "bs-toast-bg": "var(--bs-secondary)",
            "bs-toast-header-bg": "var(--bs-dark)"
        },
