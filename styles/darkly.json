        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "Lato, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
                    "name": "System",
                    "slug": "system"
                },
                {
                    "fontFamily": "Lato",
                    "slug": "lato",
                    "fontFace": [
                        {
                            "fontFamily": "Lato",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./assets/fonts/lato/lato_normal_400.ttf"
