{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"title": "Cosmo",
	"settings": {
		"custom": {
			"sp-replace-string": "all",
			"bs-body-font-size": "15px",
			"bs-html-text-shadow": "var(--bs-light-textshadow)",
			"bs-border-radius": "0",
			"bs-border-radius-sm": "0",
			"bs-border-radius-lg": "0",
			"bs-border-radius-xl": "0",
			"bs-border-radius-xxl": "0",
			"bs-btn-focus-shadow-rgb": "var(--bs-primary-rgb)",
			"bs-popover-bg": "var(--bs-secondary-bg)",
			"bs-progress-height": "var(--wp--preset--font-size--x-small)",
			"bs-progress-bg": "var(--bs-border-color)"
		},
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
