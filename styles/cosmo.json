							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "700",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/roboto/roboto_normal_700.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\",\"Courier New\",monospace",
					"name": "Bootstrap Monospace Stack",
					"slug": "bs-monospace"
				}
			]
		}
