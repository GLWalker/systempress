		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Roboto, -apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Helvetica Neue\",Arial, sans-serif, \"Apple Color Emoji\",\"Segoe UI Emoji\",\"Segoe UI Symbol\"",
					"name": "System San Serif Stack",
					"slug": "system"
				},
				{
					"fontFamily": "Roboto",
					"name": "Roboto",
					"slug": "roboto",
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "300",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/roboto/roboto_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "400",
							"fontDisplay": "swap",
