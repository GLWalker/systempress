				},
				{
					"color": "#ffce67",
					"name": "Warning",
					"slug": "bs-warning"
				},
				{
					"color": "#ff7851",
					"name": "Danger",
					"slug": "bs-danger"
				},
				{
					"color": "#f8f9fa",
					"name": "Light",
					"slug": "bs-light"
				},
				{
					"color": "#343a40",
					"name": "Dark",
