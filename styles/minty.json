	"styles": {
		"css": "body {\n  --bs-primary-text: #fff !important;\n  --bs-secondary-text: #fff !important;\n}\n.navbar, .nav, .btn, .wp-element-button, .pagination, .wp-block-query-pagination,legend {\n.breadcrumb a {\n  color: rgba(255, 255, 255, 0.75) !important;\n}\n.breadcrumb a:hover {\n  color: #fff !important;\n  text-decoration: none;\n}\n.table-primary {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-primary) !important;\n}\n.table-secondary {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-secondary) !important;\n}\n.table-success {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-success) !important;\n}\n.table-info {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-info) !important;\n}\n.table-warning {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-warning) !important;\n}\n.table-danger {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-danger) !important;\n}\n.table-light {\n  --bs-table-bg: var(--bs-light) !important;\n}\n.table-dark {\n  --bs-table-color: #fff !important;\n  --bs-table-bg: var(--bs-dark) !important;;\n}\n.table-primary:hover, .table-secondary:hover,\n.table-success:hover, .table-info:hover, .table-warning:hover, .table-danger:hover, .table-dark:hover {\n  --bs-table-hover-color: #fff !important;\n}"
	}
}