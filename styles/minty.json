				},
				{
					"color": "#f8f9fa",
					"name": "Tertiary Element BG",
					"slug": "bs-tertiary-bg"
				},
				{
					"color": "#72b8a4",
					"name": "Border Color",
					"slug": "bs-border-color"
				},
				{
					"color": "#78c2ad",
					"name": "Primary",
					"slug": "bs-primary"
				},
				{
					"color": "#f3969a",
					"name": "Secondary",
					"slug": "bs-secondary"
				},
				{
					"color": "#56cc9d",
					"name": "Success",
					"slug": "bs-success"
				},
				{
					"color": "#6cc3d5",
					"name": "Info",
