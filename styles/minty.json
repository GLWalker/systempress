{
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"title": "Minty",
	"settings": {
		"custom": {
			"sp-replace-string": "all",
			"bs-border-width": "1px",
			"bs-border-style": "solid",
			"bs-border-radius": "0.4rem",
			"bs-border-radius-sm": "0.3rem",
			"bs-border-radius-lg": "0.6rem",
			"bs-border-radius-xl": "1rem",
			"bs-border-radius-xxl": "2rem",
			"bs-sp-spacing-x": "0.75rem",
			"bs-sp-spacing-y": "0.375rem",
			"bs-btn-font-weight": "400",
			"bs-btn-line-height": "1.5",
			"bs-btn-border-width:": "var(--bs-border-width)",
			"bs-btn-radius": "var(--bs-border-radius)",
			"bs-btn-radius-sm": "var(--bs-border-radius-sm)",
			"bs-btn-radius-lg": "var(--bs-border-radius-lg)",
			"bs-pagination-color": "var(--bs-primary-text)",
			"bs-pagination-bg": "var(--bs-primary)",
			"bs-pagination-border-color": "var(--bs-primary-border-color)",
			"bs-pagination-hover-color": "var(--bs-secondary-text)",
			"bs-pagination-hover-bg": "var(--bs-secondary)",
			"bs-pagination-hover-border-color": "var(--bs-secondary-border-color)",
			"bs-pagination-active-color": "var(--bs-secondary-text)",
			"bs-pagination-active-bg": "var(--bs-secondary)",
			"bs-pagination-active-border-color": "var(--bs-secondary)",
			"bs-breadcrumb-bg": "var(--bs-primary)",
			"bs-breadcrumb-divider-color": "var(--bs-primary-text)",
			"bs-breadcrumb-item-active-color": "var(--bs-primary-text)"
		},
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#888888",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgba(136, 136, 136, 0.75)",
					"name": "Secondary Element Text",
					"slug": "bs-secondary-color"
				},
				{
					"color": "#f7f7f9",
					"name": "Secondary Element BG",
					"slug": "bs-secondary-bg"
				},
				{
					"color": "rgba(136, 136, 136, 0.5)",
					"name": "Tertiary Element Text",
